  <rdf:Description rdf:about="http://www.w3.org/ns/person#Person">
    <rdfs:label xml:lang="en">Person</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/person#Person">
    <rdfs:comment xml:lang="en">An individual person who may be dead or alive, but not imaginary. It is that restriction that makes person:Person a sub class of both foaf:Person and schema:Person which both cover imaginary characters as well as real people.</rdfs:comment>
  </rdf:Description>
